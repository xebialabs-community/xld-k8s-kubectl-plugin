
{
  "kind": "Deployment",
  "apiVersion": "apps/v1",
  "metadata": {
    "name": "back-deployment-0.0.81",
    "labels": {
      "application": "xl-front-back-app",
      "component": "back"
    },
    "annotations": {
      "xldeploy.com/version": "0.0.81",
      "xldeploy.com/environment": "xl-demo-{{env}}"
    }
  },
  "spec": {
    "replicas": 1,
    "strategy": {
      "type":"RollingUpdate",
      "rollingUpdate": {
        "maxSurge": 1,
        "maxUnavailable": 0
      }
    },
    "selector": {
      "matchLabels": {
        "application": "xl-front-back-app",
        "component": "back"
      }
    },
    "template": {
      "metadata": {
        "name": "back-deployment-0.0.81",
        "labels": {
          "application": "xl-front-back-app",
          "component": "back",
          "version": "0.0.81"
        }
      },
      "spec": {
        "volumes": [
        ],
        "imagePullSecrets": [
        ],
        "containers": [
          {
            "name": "back",
            "image": "bmoussaud/xl-demo-backend:1.0.81",
            "ports": [
              {
                "containerPort": 8000,
                "protocol": "TCP"
              }
                          ],
             "env": [
                              
                                ],
            "volumeMounts": [
             ],
            "terminationMessagePath": "/dev/termination-log",
            "terminationMessagePolicy": "File",
            "imagePullPolicy": "IfNotPresent"
          }
        ],
        "restartPolicy": "Always",
        "terminationGracePeriodSeconds": 30
      }
    }
  }
}




